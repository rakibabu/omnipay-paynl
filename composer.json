{
    "name": "paynl/omnipay-paynl",
    "description": "Pay.nl driver for the Omnipay PHP payment processing library",
    "type": "library",
    "require": {
        "omnipay/common": "~3",
        "ext-json": "*"
    },
    "require-dev": {
        "omnipay/tests": "~3",
      "monolog/monolog": "^1.23",
      "php-coveralls/php-coveralls": "^2.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "andypieters",
            "email": "andy@pay.nl"
        }
    ],
    "autoload": {
        "psr-4": { "Omnipay\\Paynl\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Omnipay\\Paynl\\Test\\": "tests/" }
    }
}
